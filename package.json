{
  "name": "nishan",
  "version": "0.0.0",
  "description": "Unofficial Notion Api wrapper for Node.js",
  "main": "./dist/Nishan",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "start": "tsc --build tsconfig.json --watch",
    "prebuild": "npm run lint && run-s clean",
    "prestart": "run-s clean",
    "clean": "del-cli packages/*/dist",
    "dev": "run-s start",
    "pretest": "run-s build",
    "test": "run-s test:*",
    "test:unit": "lerna run test",
    "bootstrap": "lerna bootstrap",
    "publish": "lerna publish",
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') < 0) throw new Error('this package requires yarn for development')\"",
    "postinstall": "run-s bootstrap",
    "lint": "npx eslint . --color"
  },
  "keywords": [],
  "author": "Safwan Shaheer <devorein00@gmail.com>",
  "license": "MIT"
}