{
  "name": "nishan",
  "version": "0.0.0",
  "description": "Unofficial Notion Api wrapper for Node.js",
  "main": "./dist/Nishan",
  "dependencies": {},
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "npm run clean && npx tsc --removeComments",
    "watch": "npx tsc --incremental true -w",
    "clean": "npx rimraf ./dist",
    "lint": "npx eslint . --color",
    "prepack": "npm run test:build",
    "prepublishOnly": "npm run test:build",
    "test": "npx jest --runInBand ./tests",
    "test:build": "npm run test && npm run build",
    "transpile": "npx babel src --out-dir dist --extensions .ts",
    "typecheck": "npx tsc --noEmit --incremental false",
    "preversion": "npm run test"
  },
  "keywords": [],
  "author": "Safwan Shaheer <devorein00@gmail.com>",
  "license": "MIT"
}